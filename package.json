{
    "engines": {
        "node": "8.9.4"
    },
    "name": "ts-web-framework",
    "version": "1.7.4",
    "description": "A Web Server Framework ideal for create REST api's",
    "repository": {
        "type": "git",
        "url": "https://github.com/TypeScript-Web-Framework/ts-web-framework.git"
    },
    "main": "www/server.js",
    "scripts": {
        "start": "node www/server.js",
        "build": "gulp",
        "serve": "npm run build && npm run start",
        "test": "mocha www/tests/**/*.js --reporter spec",
        "pkg": "npm run build && npm test && pkg -t node8-x64 --out-path ./bin --options max_old_space_size=4096 .",
        "pkg32": "npm run build && npm test && pkg -t node8-x32 --out-path ./bin --options max_old_space_size=4096 .",
        "creator": "./bin/",
        "pretest" : "npm run build"
    },
    "author": "Olaf Erlandsen C.",
    "contributors": [
        {
            "name": "Olaf Erlandsen C.",
            "email": "olaftriskel@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "ts",
        "web",
        "framework",
        "http",
        "server",
        "api",
        "rest"
    ],
    "bugs": {
        "url": "https://github.com/TypeScript-Web-Framework/ts-web-framework/issues",
        "email": "olaftriskel@gmail.com"
    },
    "homepage": "https://github.com/TypeScript-Web-Framework/ts-web-framework",
    "dependencies": {
        "@types/csurf": "^1.9.35",
        "@types/helmet": "0.0.38",
        "@types/lodash": "^4.14.108",
        "@types/ua-parser-js": "^0.7.32",
        "body-parser": "^1.15.2",
        "cookie-parser": "^1.4.3",
        "cookie-session": "^2.0.0-beta.3",
        "cors": "^2.8.4",
        "csurf": "^1.9.0",
        "debug": "^3.1.0",
        "express": "^4.14.0",
        "express-session": "^1.15.6",
        "gulp": "^4.0.0",
        "helmet": "^3.12.1",
        "lodash": "^4.17.10",
        "moment": "^2.22.1",
        "morgan": "^1.7.0",
        "patch": "0.0.1",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.2.0",
        "snyk": "1.90.1",
        "supertest": "^3.1.0",
        "ua-parser-js": "^0.7.17"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/chai": "^4.1.3",
        "@types/debug": "0.0.30",
        "@types/express": "^4.0.33",
        "@types/mocha": "^5.2.0",
        "@types/morgan": "^1.7.32",
        "@types/node": "^10.5.2",
        "@types/rx": "^4.1.1",
        "chai": "^4.1.2",
        "commander": "^2.16.0",
        "gulp-clean": "^0.4.0",
        "gulp-minify": "^3.1.0",
        "gulp-remove-logging": "^1.2.0",
        "gulp-rm": "^1.0.5",
        "gulp-typescript": "^4.0.1",
        "mocha": "^5.2.0",
        "typescript": "^2.8.3"
    },
    "pkg": {
        "scripts": "www/**/*.js",
        "assets": "www/**/*.json"
    },
    "bin": "www/server.js"
}
